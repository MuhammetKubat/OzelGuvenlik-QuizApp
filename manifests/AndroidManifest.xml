<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nmproject.quizapp.myoggproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/quiz_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/quiz_rounded"
        android:supportsRtl="true"
        android:theme="@style/Theme.QUIZAPP">
        <activity
            android:name="com.nmproject.quizapp.myoggproject.ItemsActivity"
            android:screenOrientation="fullSensor"
            android:exported="false" />
        <activity
            android:name="com.nmproject.quizapp.myoggproject.StartActivity"
            android:screenOrientation="fullSensor"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nmproject.quizapp.myoggproject.ResultActivity"
            android:screenOrientation="fullSensor"
            android:exported="false" />
        <activity
            android:name="com.nmproject.quizapp.myoggproject.MainActivity"
            android:screenOrientation="fullSensor"
            android:exported="false" />
    </application>

</manifest>